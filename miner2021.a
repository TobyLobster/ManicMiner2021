    rol                                 ;
    rol                                 ;
    rol                                 ;
    sta singleItemType                  ;
    inc singleItemType                  ; singleItemType = (top two bits / 64) + 1
    lda #$f0                            ;
    sta singleItemSprite                ; singleItemSprite = $f0
    jsr setSingleOffsetX5               ; singleItemOffsetX = 5, type++
    ldx #0                              ;
    jmp decodeNextSingleItem            ;

