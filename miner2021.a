    lda isJumping                   ;
    cmp #1                          ;
    bne notInAir                    ; if (not in air) then branch
    lda jumpPhase                   ;
    cmp #1                          ;
    bne notInAir                    ;

    lda #$04                        ; }
    ldx #$64                        ; }
    ldy #$02                        ; } play jump sound
    jmp playSoundAXY                ; }

notInAir
    lda isJumping                   ;
    cmp #1                          ;
    beq return5                     ;
    lda willyIsOnGround             ;
    cmp #0                          ;
    bne return5                     ;

    lda #$ff                        ;
    sta oswordSoundBlock + 3        ;

    lda newPlayerPixelY             ; }
    sec                             ; }
    sbc playerHeightOnTakeOff       ; }
    asl                             ; }
    eor #$7f                        ; } play descending falling sound
    tax                             ; }
    lda #$f1                        ; }
    ldy #1                          ; }
    jsr playSoundAXY                ; }

    lda #0                          ;
    sta oswordSoundBlock + 3        ;
return5
    rts                             ;

