    ; $0090
zeroScore
    ; clear the score (6 digits in 3 BCD bytes)
    lda #0                          ;
    tax                             ;
-
    sta score, x                    ;
    inx                             ;
    cpx #3                          ;
    bne -                           ;
    jmp miner3EntryPoint            ;

    !byte $ff, $00                  ; [UNUSED]
}
