; ***************************************************************************************
;
; Reverse the order of all bits in A
;
; [ This is not the most efficient way of doing it
;   e.g. use this from https://sites.google.com/site/h2obsession/programming/6502
; reverseBits
;     sta byteToReverse               ;
;     lda #1                          ;
; -
;     lsr byteToReverse               ;
;     rol                             ;
;     bcc -                           ;
;     rts                             ;
; ]

; ***************************************************************************************
reverseBits
    sta byteToReverse               ;

    ldy #0                          ;
    ldx #0                          ;
reverseBitsLoop
    lda byteToReverse               ;
    clc                             ;
    rol                             ;
    sta byteToReverse               ;
    bcs bitSet                      ;
    txa                             ;
    clc                             ;

continueReverseBits
    ror                             ;
    tax                             ;
    iny                             ;
    cpy #8                          ;
    bne reverseBitsLoop             ;
    rts                             ;

bitSet
    txa                             ;
    sec                             ;
    jmp continueReverseBits         ;

