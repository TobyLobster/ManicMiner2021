    clc                                 ;
    adc #4                              ;
    sec                                 ;
tempScreenByte = * + 1
    sbc #$ff                            ; playerX + 4 - (guardianX + 5) * 4
    cmp #0                              ;
    beq checkVerticalCollision          ;
    cmp #7                              ;
    bcc checkVerticalCollision          ;
    sec                                 ;
    sbc collisionFudgeFactor            ; subtract 0 or 1
    cmp #7                              ;
    bcc checkVerticalCollision          ;
    clc                                 ;
    adc collisionFudgeFactor            ; subtract 0 or 1
    cmp #$fd                            ;
    bcs checkVerticalCollision          ;
    clc                                 ;
    adc collisionFudgeFactor            ; subtract 0 or 1
    cmp #$fd                            ;
    bcs checkVerticalCollision          ;
return9
    rts                                 ;

