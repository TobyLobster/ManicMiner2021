}
    lda zpGuardianPositionY         ;
    eor #$40                        ; flip bit 6
    sta zpGuardianPositionY         ;

    tay                             ;
    and #$20                        ;
    cmp #$20                        ;
    bne moveLeftOrRight             ; if (bit 5 clear) then branch
    tya                             ;
