    asl                             ;
    sta zpGuardianAnimIndex         ; temp = 2 * (guardianX and 3)
    bne notAtCellXBoundary          ;

    jsr positionCursorAtGuardian    ;
    jsr eraseLeftOfGuardian         ;

notAtCellXBoundary
