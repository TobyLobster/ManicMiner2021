    ; or   f1+temp, f0+temp
    ;      f9+temp, f8+temp
    lda spriteToDraw,x              ;
    clc                             ;
    adc zpGuardianAnimIndex         ;
    jsr drawIfNotOverExit           ;

    lda spriteToDraw + 1,x          ;
    clc                             ;
    adc zpGuardianAnimIndex         ;
    jsr drawIfNotOverExit           ;

