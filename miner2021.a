    JSR .getBitMask                     ;
.compare1 = * + 1
    AND #$FF                            ;
    BNE .foundCollision                 ;
    INC colSprite1Offset                ;
    INC colSprite2Offset                ;
    DEC collisionLoopCounter            ;
    BNE .checkCollisionLoop             ;
+
    RTS                                 ;

; ***************************************************************************************
.foundCollision
    LDA #255                            ; Found a collision
    STA gotCollision                    ;
    RTS                                 ;

; ***************************************************************************************
.checkAD
; On Entry:
;     sprite1OffsetWithinCell  is the byte offset (0-7) to start checking rows in sprite1
;     sprite2OffsetWithinCell  is the byte offset (0-7) to start checking rows in sprite2 (second half)
;     checkLength  is the number of consecutive rows to check
    LDA collisionSprite1Height          ;
    CMP #1                              ;
    BEQ +                               ; if(sprite1 is height 1) then branch (no collision)
    LDA sprite2OffsetWithinCell         ;
    CLC                                 ;
    ADC collisionSprite2Stride          ;
    STA sprite2OffsetWithinCell         ;
    JSR .checkCollisionRows             ;
    LDA sprite2OffsetWithinCell         ;
    SEC                                 ;
    SBC collisionSprite2Stride          ;
    STA sprite2OffsetWithinCell         ;
+
    RTS

; ***************************************************************************************
.checkBC
; On Entry:
;     sprite1OffsetWithinCell  is the byte offset (0-7) to start checking rows in sprite1 (second half)
;     sprite2OffsetWithinCell  is the byte offset (0-7) to start checking rows in sprite2
;     checkLength  is the number of consecutive rows to check
    LDA collisionSprite2Height          ;
    CMP #1                              ;
    BEQ +                               ; if(sprite2 is height 1) then branch (no collision)
    LDA sprite1OffsetWithinCell         ;
    CLC                                 ;
    ADC collisionSprite1Stride          ;
    STA sprite1OffsetWithinCell         ;
    JSR .checkCollisionRows             ;
    LDA sprite1OffsetWithinCell         ;
    SEC                                 ;
    SBC collisionSprite1Stride          ;
    STA sprite1OffsetWithinCell         ;
+
    RTS                                 ;

; ***************************************************************************************
.checkBD
; On Entry:
;     sprite1OffsetWithinCell  is the byte offset (0-7) to start checking rows in sprite1 (second half)
;     sprite2OffsetWithinCell  is the byte offset (0-7) to start checking rows in sprite2
;     checkLength  is the number of consecutive rows to check
    LDA sprite2OffsetWithinCell         ;
    CLC                                 ;
    ADC collisionSprite2Stride          ;
    STA sprite2OffsetWithinCell         ;
    LDA sprite1OffsetWithinCell         ;
    CLC                                 ;
    ADC collisionSprite1Stride          ;
    STA sprite1OffsetWithinCell         ;
    JSR .checkCollisionRows             ;
    LDA sprite1OffsetWithinCell         ;
    SEC                                 ;
    SBC collisionSprite1Stride          ;
    STA sprite1OffsetWithinCell         ;
    LDA sprite2OffsetWithinCell         ;
    SEC                                 ;
    SBC collisionSprite2Stride          ;
    STA sprite2OffsetWithinCell         ;
+
    RTS                                 ;

; ***************************************************************************************
.getBitMask
;   On Entry:
;       .spriteLow is 2 byte pointer in zero page that points to start of the sprite
;       X = the number of cells wide the sprite is (1 or 2)
;       Y = byte offset within sprite
;   On Exit:
;       A = bit mask
    LDA #0                              ;
    STA .tempStore                      ;
    CPX #1                              ;
    BEQ +                               ; branch if one cell wide
    LDA (spriteLow),Y                   ;
    ASL                                 ;
    ASL                                 ;
    ASL                                 ;
    ASL                                 ;
    ORA (spriteLow),Y                   ;
    AND #$F0                            ; Logical OR together the lower nybble and the upper nybble
    STA .tempStore                      ; Remember
    TYA                                 ;
    CLC                                 ;
    ADC #8                              ; Move on to second half of the 2 cell wide sprite
    TAY                                 ;
+
    LDA (spriteLow),Y                   ; Get the upper nybble, move it to the lower nybble
    LSR                                 ;
    LSR                                 ;
    LSR                                 ;
    LSR                                 ;
    ORA (spriteLow),Y                   ; Logical OR with the original nybble
    AND #$0F                            ;
.tempStore = * + 1
    ORA #$FF                            ; Logical OR in each nybble together
    RTS                                 ;
} ; CRAZY_COLLISION_CODE

