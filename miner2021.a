showCrumble
    ldy #7                              ; loop counter
-
    lda (plotScreenAddressLow),y        ;
    iny                                 ;
    sta (plotScreenAddressLow),y        ;
    tya                                 ;
    clc                                 ;
    adc #7                              ;
    tay                                 ;
    lda (plotScreenAddressLow),y        ;
    iny                                 ;
    sta (plotScreenAddressLow),y        ;
    tya                                 ;
    sec                                 ;
    sbc #10                             ;
    tay                                 ;
    bne -                               ;

    lda #0                              ;
    ldy #1                              ;
    sta (plotScreenAddressLow),y        ;
    ldy #9                              ;
    sta (plotScreenAddressLow),y        ;
    rts                                 ;

