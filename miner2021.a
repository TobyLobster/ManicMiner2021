+
    ldy tempLoopCounter                                 ; read modified values into Y and A
    lda tempByte                                        ;
    sta characterFromScreen,Y                           ; store copy
    dey                                                 ; and do it again
    bpl setupPatternLoop                                ; until 8 bytes copied
    rts                                                 ;

